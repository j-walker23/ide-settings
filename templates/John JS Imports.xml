<templateSet group="John JS Imports">
  <template name="exx" value="export * from './$path$'&#10;$END$" description="export * from '/'" toReformat="true" toShortenFQNames="true">
    <variable name="path" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="imp" value="import { $what$ } from '$where$'" description="import {} from 'yourmom'" toReformat="true" toShortenFQNames="true">
    <variable name="where" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="what" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="ima" value="import * as $name$ from 'app/$path$$END$'" description="import * as from" toReformat="true" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="path" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="exp" value="export { $what$ } from '$where$'" description="export {} from '.'" toReformat="true" toShortenFQNames="true">
    <variable name="where" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="what" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="imd" value="import $what$ from '$where$'" description="import $var$ from 'yourmom'" toReformat="true" toShortenFQNames="true">
    <variable name="where" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="what" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="imdd" value="import $END$$what$ from '$where$'" description="import camelCase('yourmom') from 'yourmom'" toReformat="true" toShortenFQNames="true">
    <variable name="where" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="what" expression="camelCase(where)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="imt" value="import template from './$where$.html'" description="import template from './suckit.html'" toReformat="true" toShortenFQNames="true">
    <variable name="where" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>